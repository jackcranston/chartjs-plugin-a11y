{
  "name": "chartjs-plugin-a11y",
  "version": "0.1.2",
  "description": "Accessibility plugin for Chart.js",
  "license": "MIT",
  "type": "module",
  "main": "./dist/chartjs-plugin-a11y.cjs",
  "module": "./dist/chartjs-plugin-a11y.js",
  "exports": {
    ".": {
      "import": "./dist/chartjs-plugin-a11y.js",
      "require": "./dist/chartjs-plugin-a11y.cjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "chartjs",
    "chart.js",
    "plugin",
    "a11y",
    "accessibility"
  ],
  "scripts": {
    "start": "browser-sync start --server --index ./demo/index.html",
    "autobuild": "rollup -c -w",
    "build": "rollup -c",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "chart.js": "^4.2.1",
    "eslint": "^8.23.0",
    "eslint-config-chartjs": "^0.3.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rollup": "^3.20.2"
  }
}
